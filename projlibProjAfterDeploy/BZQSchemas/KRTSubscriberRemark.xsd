	 targetNamespace="http://www.bezeq.co.il/XMLSchema/KRTSubscriberRemark"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="KRTSubscriberRemark">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="interfaceId" type="xs:string" minOccurs="0"/>
				<xs:element name="subscriberId" type="xs:string"/>
