								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="billingServiceId" type="xs:integer"/>
										<xs:element maxOccurs="unbounded" minOccurs="0" name="serviceAttributes">
											<xs:complexType>
												<xs:sequence>
													<xs:element minOccurs="0" name="serviceAttributeName" type="xs:string"/>
													<xs:element minOccurs="0" name="serviceAttributeValue" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element minOccurs="0" name="providerData" type="xs:string"/>
										<xs:element minOccurs="0" name="ossServiceStatus" type="xs:string"/>
										<xs:element minOccurs="0" name="ossServiceReturnCode" type="xs:string"/>
										<xs:element minOccurs="0" name="ossServiceReturnDesc" type="xs:string"/>
										<xs:element minOccurs="0" name="providerServiceStatus" type="xs:string"/>
										<xs:element minOccurs="0" name="providerServiceReturnCode" type="xs:string"/>
										<xs:element minOccurs="0" name="providerServiceReturnDesc" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="status" type="xs:string"/>
							<xs:element minOccurs="0" name="returnCode" type="xs:string"/>
							<xs:element minOccurs="0" name="returnDesc" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element minOccurs="0" ref="BzqError:BzqError"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
